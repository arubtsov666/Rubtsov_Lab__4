<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

        <servlet>
            <servlet-name>addContact</servlet-name>
            <servlet-class>servlets.AddContact</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>addContact</servlet-name>
            <url-pattern>/addContact</url-pattern>
        </servlet-mapping>

        <servlet>
            <servlet-name>listContact</servlet-name>
            <servlet-class>servlets.ListContact</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>listContact</servlet-name>
            <url-pattern>/listContact</url-pattern>
        </servlet-mapping>

        <servlet>
            <servlet-name>currentContact</servlet-name>
            <servlet-class>servlets.CurrentContact</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>currentContact</servlet-name>
            <url-pattern>/CurrentContact</url-pattern>
        </servlet-mapping>

        <servlet>
            <servlet-name>likedContacts</servlet-name>
            <servlet-class>servlets.LikedContacts</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>likedContacts</servlet-name>
            <url-pattern>/likedContacts</url-pattern>
        </servlet-mapping>

        <servlet>
            <servlet-name>aboutApp</servlet-name>
            <servlet-class>servlets.AboutApp</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>aboutApp</servlet-name>
            <url-pattern>/aboutApp</url-pattern>
        </servlet-mapping>

        <servlet>
            <servlet-name>authUser</servlet-name>
            <servlet-class>servlets.AuthUser</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>authUser</servlet-name>
            <url-pattern>/authUser</url-pattern>
        </servlet-mapping>

</web-app>